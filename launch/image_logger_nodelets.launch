<launch>
	<!-- $ rosrun nodelet nodelet manager __name:=sample_nodelet_manager -->
	<node pkg="nodelet" type="nodelet" args="manager"
    name="iai_image_logging_manager" output="screen" />

  <!-- $ rosrun nodelet nodelet load sample_nodelet/SampleNodeletClass sample_nodelet_manager __name:=SampleNodeletClass_loader-->
	<node pkg="nodelet" type="nodelet" args="load iai_image_logging/ImageLogger image_logger"
    name="ImageLogger_loader" output="screen">
	</node>

	<!-- $ rosrun nodelet nodelet load sample_nodelet/SampleNodeletClass2 sample_nodelet_manager __name:=SampleNodeletClass_loader-->
	<node pkg="nodelet" type="nodelet" args="load iai_image_logging/Preprocessor preprocessor"
    name="Preprocessor_loader" output="screen">
	</node>
</launch>